{
  "name": "pathfinder",
  "version": "0.1.0",
  "private": true,
  "browserslist": {
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ],
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ]
  },
  "scripts": {
    "build": "npm run build:wasm && npx next build",
    "build:wasm": "cd pathfinder && PATH=$HOME/.cargo/bin:$PATH wasm-pack build --target web --out-dir pkg",
    "build:rs": "curl https://sh.rustup.rs -sSf | sh -s -- -y && (cd pathfinder && npm install && PATH=$HOME/.cargo/bin:$PATH npm run build)",
    "dev": "next dev"
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "dependencies": {
    "@mapbox/polyline": "^1.2",
    "@radix-ui/react-dialog": "^1.1",
    "@radix-ui/react-icons": "^1.3",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-popover": "^1",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.1",
    "@radix-ui/react-slot": "^1.1",
    "@radix-ui/react-tabs": "^1.1.13",
    "@tmcw/togeojson": "^7.1.2",
    "@turf/turf": "^7.1",
    "@types/leaflet": "^1.9",
    "chart.js": "^4.4",
    "chartjs-adapter-date-fns": "^3",
    "chartjs-plugin-zoom": "^2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1",
    "cmdk": "^1.0.4",
    "date-fns": "^3.6.0",
    "file-saver": "^2.0.5",
    "georaster": "^1.6.0",
    "georaster-layer-for-leaflet": "^3.10.0",
    "geotiff": "^2.1.3",
    "leaflet": "^1.9",
    "loglevel": "^1.9",
    "lucide-react": "^0.453",
    "next": "^15.1",
    "react": "^19.0",
    "react-chartjs-2": "^5.3",
    "react-dom": "^19.0",
    "react-icons": "^5.5.0",
    "react-leaflet": "v5.0.0-rc.1",
    "sonner": "^1.7",
    "tailwind-merge": "^2.5",
    "togpx": "^0.5.4",
    "zod": "^3.23",
    "zustand": "^5"
  },
  "devDependencies": {
    "@biomejs/biome": "^1",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "postcss": "^8",
    "tailwindcss": "^3.4",
    "tailwindcss-animate": "^1.0",
    "typescript": "^5"
  }
}
